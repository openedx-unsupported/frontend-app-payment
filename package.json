{
  "name": "@edx/frontend-app-payment",
  "version": "0.1.0",
  "description": "Payment React app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-app-payment.git"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=production node build.prod.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "i18n_extract": "BABEL_ENV=i18n babel src --quiet > /dev/null",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "eslint --ext .js --ext .jsx .",
    "precommit": "make precommit",
    "snapshot": "jest --updateSnapshot",
    "start": "NODE_ENV=development BABEL_ENV=development webpack-dev-server --config=webpack/webpack.dev.config.js --progress",
    "start:stage": "NODE_ENV=development BABEL_ENV=development webpack-dev-server --config=webpack/webpack.dev-stage.config.js --progress --disable-host-check",
    "test": "jest --coverage --passWithNoTests",
    "travis-deploy-once": "travis-deploy-once"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edx/frontend-app-payment#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@cospired/i18n-iso-languages": "2.0.5",
    "@edx/edx-bootstrap": "2.2.2",
    "@edx/frontend-analytics": "3.0.0",
    "@edx/frontend-auth": "7.0.1",
    "@edx/frontend-component-site-header": "2.5.0",
    "@edx/frontend-i18n": "3.0.3",
    "@edx/frontend-logging": "3.0.1",
    "@edx/paragon": "7.1.4",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-regular-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.5",
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.6",
    "form-urlencoded": "4.0.0",
    "glob": "7.1.4",
    "history": "4.10.1",
    "lodash.camelcase": "4.3.0",
    "lodash.get": "4.4.2",
    "lodash.pick": "4.4.0",
    "lodash.snakecase": "4.1.1",
    "lodash.without": "4.4.0",
    "newrelic": "5.11.0",
    "prop-types": "15.7.2",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-redux": "7.1.1",
    "react-router": "5.1.1",
    "react-router-dom": "5.1.1",
    "react-tooltip": "3.11.1",
    "react-transition-group": "4.3.0",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-form": "8.2.6",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.1",
    "redux-saga-routines": "3.2.1",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "universal-cookie": "4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@svgr/webpack": "4.3.3",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-react-intl": "4.1.19",
    "babel-plugin-transform-imports": "2.0.0",
    "chance": "1.1.0",
    "clean-webpack-plugin": "3.0.0",
    "codecov": "3.6.1",
    "copy-webpack-plugin": "5.0.4",
    "css-loader": "3.2.0",
    "cssnano": "4.1.10",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "es-check": "5.0.0",
    "eslint-config-edx": "4.0.4",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.7",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "6.0.0",
    "jest": "24.9.0",
    "lodash.defaultsdeep": "4.6.1",
    "mini-css-extract-plugin": "0.8.0",
    "new-relic-source-map-webpack-plugin": "1.2.0",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "postcss-rtl": "1.5.0",
    "purgecss-webpack-plugin": "1.6.0",
    "react-axe": "3.3.0",
    "react-dev-utils": "9.0.4",
    "react-test-renderer": "16.10.1",
    "reactifex": "1.1.1",
    "redux-mock-store": "1.5.3",
    "rosie": "2.0.1",
    "sass-loader": "8.0.0",
    "source-map-loader": "0.2.4",
    "style-loader": "1.0.0",
    "travis-deploy-once": "5.0.11",
    "webpack": "4.41.0",
    "webpack-bundle-analyzer": "3.5.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.1",
    "webpack-merge": "4.2.2"
  },
  "jest": {
    "testURL": "http://localhost/",
    "setupFiles": [
      "./src/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgrMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "src/setupTest.js",
      "src/index.jsx",
      "src/common/components",
      "src/common/card-validator",
      "src/common/actions.js",
      "src/common/serviceUtils.js",
      "src/components",
      "src/i18n",
      "src/store",
      "environment.js",
      "src/data"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@edx/paragon).+\\.js$"
    ]
  }
}
