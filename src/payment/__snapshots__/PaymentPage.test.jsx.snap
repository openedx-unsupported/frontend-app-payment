    <h1
      className="sr-only"
    >
      <span>
        Payment
      </span>
    </h1>
    <div
      className="col-md-5 pr-md-5 col-basket-summary"
    >
      <section
        aria-label="Shopping Cart Details"
        aria-live="polite"
        aria-relevant="all"
      >
        <div>
          <span
            className="sr-only"
          >
            <span>
              Shopping cart details are loaded.
            </span>
          </span>
          <div
            className="basket-section"
          >
            <h5
              aria-level="2"
            >
              <span>
                In Your Cart
              </span>
            </h5>
            <p>
              <span>
                Your purchase contains the following:
              </span>
            </p>
          </div>
          <div
            aria-labelledby="summary-heading"
            aria-live="polite"
            className="basket-section"
            role="region"
          >
            <h5
              aria-level="2"
              id="summary-heading"
            >
              <span>
                Summary
              </span>
            </h5>
            <div
              className="summary-row d-flex"
            >
              <span
                className="flex-grow-1"
              >
                <span>
                  Price
                </span>
              </span>
              <span />
            </div>
            <div
              className="summary-row font-weight-bold d-flex"
            >
              <span
                className="flex-grow-1"
              >
                <span>
                  TOTAL
                </span>
              </span>
              <span
                className="text-right"
              />
            </div>
          </div>
        </div>
      </section>
    </div>
    <div
      className="col-md-7 pl-md-5"
    >
      <section
        aria-label="Payment Details"
      >
        <div
          className="basket-section"
        >
          <h5
            aria-level="2"
          >
            <span>
              Select Payment Method
            </span>
          </h5>
          <p
            className="d-flex flex-wrap"
          >
            <button
              className="payment-method-button active"
            >
              <img
                alt="Credit Card"
                src="test-file-stub"
              />
            </button>
            <button
              className="payment-method-button"
              disabled={false}
              onClick={[Function]}
            >
              <img
                alt="PayPal"
                src="test-file-stub"
              />
            </button>
          </p>
        </div>
        <form
